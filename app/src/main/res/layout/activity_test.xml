<?xml version="1.0" encoding="utf-8"?>
<de.dlyt.yanndroid.samsung.layout.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dview"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:drawer_icon="@drawable/ic_samsung_info"
    app:drawer_viewId="@id/ddyalla"
    app:toolbar_subtitle="subtitle test"
    app:toolbar_title="Test"
    tools:context=".TestActivity">


    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/ddyalla"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="this view should be in the drawer" />


    <include layout="@layout/content_view" />

</de.dlyt.yanndroid.samsung.layout.DrawerLayout>


